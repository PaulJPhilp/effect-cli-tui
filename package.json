{
  "name": "effect-cli-tui",
  "version": "1.0.0",
  "description": "Effect-native CLI wrapper with interactive prompts and Ink components for terminal UIs",
  "keywords": [
    "effect",
    "cli",
    "tui",
    "terminal",
    "prompts",
    "interactive",
    "ink",
    "react"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulJPhilp/effect-cli-tui.git"
  },
  "homepage": "https://github.com/PaulJPhilp/effect-cli-tui",
  "bugs": {
    "url": "https://github.com/PaulJPhilp/effect-cli-tui/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src __tests__",
    "format": "prettier --write src __tests__ docs",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "dependencies": {
  "chalk": "^5.3.0",
  "cli-spinners": "^3.0.0",
  "cli-table3": "^0.6.3",
  "effect": "^3.18.0",
  "highlight.js": "^11.9.0"
  },
  "peerDependencies": {
    "ink": "^4.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.0",
    "vitest": "^4.0.6"
  },
  "engines": {
    "node": ">=20"
  }
}
