{
  "name": "effect-cli-tui",
  "version": "1.0.0",
  "description": "Effect-native CLI wrapper with interactive prompts and Ink components for terminal UIs",
  "keywords": [
    "effect",
    "cli",
    "tui",
    "terminal",
    "prompts",
    "interactive",
    "ink",
    "react"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/effect-cli-tui.git"
  },
  "homepage": "https://github.com/PaulJPhilp/effect-cli-tui",
  "bugs": {
    "url": "https://github.com/PaulJPhilp/effect-cli-tui/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src __tests__",
    "format": "prettier --write src __tests__ docs",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun run test"
  },
  "type": "module",
  "dependencies": {
    "@effect/schema": "^0.75.5",
    "@inquirer/prompts": "^5.5.0",
    "chalk": "^5.6.2",
    "cli-spinners": "^3.3.0",
    "cli-table3": "^0.6.5",
    "effect-cli": "^1.0.0",
    "highlight.js": "^11.11.1"
  },
  "peerDependencies": {
    "effect": "^3.0.0",
    "ink": "^4.4.1",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.26",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "effect": "^3.19.3",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=20"
  }
}
