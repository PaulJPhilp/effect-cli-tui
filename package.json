{
  "name": "effect-cli-tui",
  "version": "2.0.1",
  "description": "Effect-native CLI wrapper with interactive prompts and Ink components for terminal UIs",
  "keywords": [
    "effect",
    "cli",
    "tui",
    "terminal",
    "prompts",
    "interactive",
    "ink",
    "react"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/effect-cli-tui.git"
  },
  "homepage": "https://github.com/PaulJPhilp/effect-cli-tui",
  "bugs": {
    "url": "https://github.com/PaulJPhilp/effect-cli-tui/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "import": "./dist/theme.js"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "import": "./dist/services.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run --config vitest.config.mjs",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:real-cli": "vitest run --config vitest.real-cli.config.mjs",
    "lint": "biome check src __tests__",
    "format": "biome format src __tests__ docs",
    "type-check": "tsc --noEmit",
    "docs:generate": "typedoc --plugin typedoc-plugin-markdown --entryPointStrategy resolve --out docs/api src/index.ts src/components.ts src/theme.ts src/services.ts src/constants.ts src/types.ts",
    "prepublishOnly": "bun run build && bun run test"
  },
  "type": "module",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.68",
    "@effect/schema": "^0.75.5",
    "ai": "^5.0.95",
    "chalk": "^5.6.2",
    "cli-spinners": "^3.3.0",
    "effect-cli": "^1.0.0",
    "effect-env": "^0.4.1",
    "effect-json": "^0.1.0",
    "effect-supermemory": "^0.1.0",
    "highlight.js": "^11.11.1",
    "ink": "^4.4.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "pastel": "^3.0.0",
    "react": "^18.3.1",
    "string-width": "5.1.2",
    "table": "^6.9.0"
  },
  "peerDependencies": {
    "effect": "^3.19.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@types/ink-select-input": "^3.0.5",
    "@types/node": "^20.19.25",
    "@types/react": "^18.3.27",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "effect": "^3.19.4",
    "ink-testing-library": "^4.0.0",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "ultracite": "^6.3.4",
    "vitest": "^4.0.10"
  },
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false
}
